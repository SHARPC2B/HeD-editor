Prefix(:=<http://www.semanticweb.org/owl/owlapi/turtle#>)
Prefix(dc:=<http://purl.org/dc/elements/1.1/>)
Prefix(owl:=<http://www.w3.org/2002/07/owl#>)
Prefix(rdf:=<http://www.w3.org/1999/02/22-rdf-syntax-ns#>)
Prefix(xml:=<http://www.w3.org/XML/1998/namespace>)
Prefix(xsd:=<http://www.w3.org/2001/XMLSchema#>)
Prefix(dcam:=<http://purl.org/dc/dcam/>)
Prefix(rdfs:=<http://www.w3.org/2000/01/rdf-schema#>)
Prefix(skos:=<http://www.w3.org/2004/02/skos/core#>)
Prefix(dcterms:=<http://purl.org/dc/terms/>)
Prefix(dcmitype:=<http://purl.org/dc/dcmitype/>)


Ontology(<http://purl.org/NET/dc_owl2dl/>
Annotation(rdfs:comment "OWL 2 DL ontology for Dublin Core metadata terms from the http://purl.org/dc/terms/ namespace; this version uses object and datatype properties instead of annotation properties where possible. Note that, because of this, when using this ontology you are restricted to using literals and non-literals in certain cases and especially you cannot use DCMI's DCSV."@en)
Annotation(rdfs:label "DCMI metadata terms"@en)

Declaration(Class(dcam:VocabularyEncodingScheme))
AnnotationAssertion(rdfs:isDefinedBy dcam:VocabularyEncodingScheme <http://dublincore.org/documents/2007/06/04/abstract-model/>)
AnnotationAssertion(skos:definition dcam:VocabularyEncodingScheme "An enumerated set of resources."@en-us)
AnnotationAssertion(rdfs:label dcam:VocabularyEncodingScheme "Vocabulary Encoding Scheme"@en-us)
Declaration(Class(dcmitype:Collection))
Declaration(Class(dcmitype:Dataset))
Declaration(Class(dcmitype:Event))
Declaration(Class(dcmitype:InteractiveResource))
Declaration(Class(dcmitype:PhysicalObject))
Declaration(Class(dcmitype:Service))
Declaration(Class(dcmitype:Sound))
Declaration(Class(dcterms:Agent))
Declaration(Class(dcterms:AgentClass))
AnnotationAssertion(rdfs:comment dcterms:AgentClass "Examples of Agent Class include groups seen as classes, such as students, women, charities, lecturers."@en-us)
AnnotationAssertion(skos:definition dcterms:AgentClass "A group of agents."@en-us)
AnnotationAssertion(rdfs:label dcterms:AgentClass "Agent Class"@en-us)
Declaration(Class(dcterms:BibliographicResource))
AnnotationAssertion(skos:definition dcterms:BibliographicResource "A book, article, or other documentary resource."@en-us)
AnnotationAssertion(rdfs:label dcterms:BibliographicResource "Bibliographic Resource"@en-us)
Declaration(Class(dcterms:FileFormat))
AnnotationAssertion(rdfs:comment dcterms:FileFormat "Examples include the formats defined by the list of Internet Media Types."@en-us)
AnnotationAssertion(rdfs:label dcterms:FileFormat "File Format"@en-us)
AnnotationAssertion(skos:definition dcterms:FileFormat "A digital resource format."@en-us)
SubClassOf(dcterms:FileFormat dcterms:MediaType)
Declaration(Class(dcterms:Frequency))
AnnotationAssertion(rdfs:label dcterms:Frequency "Frequency"@en-us)
AnnotationAssertion(skos:definition dcterms:Frequency "A rate at which something recurs."@en-us)
Declaration(Class(dcterms:Jurisdiction))
AnnotationAssertion(rdfs:label dcterms:Jurisdiction "Jurisdiction"@en-us)
AnnotationAssertion(skos:definition dcterms:Jurisdiction "The extent or range of judicial, law enforcement, or other authority."@en-us)
SubClassOf(dcterms:Jurisdiction dcterms:LocationPeriodOrJurisdiction)
Declaration(Class(dcterms:LicenseDocument))
AnnotationAssertion(skos:definition dcterms:LicenseDocument "A legal document giving official permission to do something with a Resource."@en-us)
AnnotationAssertion(rdfs:label dcterms:LicenseDocument "License Document"@en-us)
SubClassOf(dcterms:LicenseDocument dcterms:RightsStatement)
Declaration(Class(dcterms:LinguisticSystem))
AnnotationAssertion(skos:definition dcterms:LinguisticSystem "A system of signs, symbols, sounds, gestures, or rules used in communication."@en-us)
AnnotationAssertion(rdfs:comment dcterms:LinguisticSystem "Examples include written, spoken, sign, and computer languages."@en-us)
AnnotationAssertion(rdfs:label dcterms:LinguisticSystem "Linguistic System"@en-us)
Declaration(Class(dcterms:Location))
AnnotationAssertion(skos:definition dcterms:Location "A spatial region or named place."@en-us)
AnnotationAssertion(rdfs:label dcterms:Location "Location"@en-us)
SubClassOf(dcterms:Location dcterms:LocationPeriodOrJurisdiction)
Declaration(Class(dcterms:LocationPeriodOrJurisdiction))
AnnotationAssertion(rdfs:label dcterms:LocationPeriodOrJurisdiction "Location, Period, or Jurisdiction"@en-us)
AnnotationAssertion(skos:definition dcterms:LocationPeriodOrJurisdiction "A location, period of time, or jurisdiction."@en-us)
Declaration(Class(dcterms:MediaType))
AnnotationAssertion(rdfs:label dcterms:MediaType "Media Type"@en-us)
AnnotationAssertion(skos:definition dcterms:MediaType "A file format or physical medium."@en-us)
SubClassOf(dcterms:MediaType dcterms:MediaTypeOrExtent)
Declaration(Class(dcterms:MediaTypeOrExtent))
AnnotationAssertion(rdfs:label dcterms:MediaTypeOrExtent "Media Type or Extent"@en-us)
AnnotationAssertion(skos:definition dcterms:MediaTypeOrExtent "A media type or extent."@en-us)
Declaration(Class(dcterms:MethodOfAccrual))
AnnotationAssertion(rdfs:label dcterms:MethodOfAccrual "Method of Accrual"@en-us)
AnnotationAssertion(skos:definition dcterms:MethodOfAccrual "A method by which resources are added to a collection."@en-us)
Declaration(Class(dcterms:MethodOfInstruction))
AnnotationAssertion(skos:definition dcterms:MethodOfInstruction "A process that is used to engender knowledge, attitudes, and skills."@en-us)
AnnotationAssertion(rdfs:label dcterms:MethodOfInstruction "Method of Instruction"@en-us)
Declaration(Class(dcterms:PeriodOfTime))
AnnotationAssertion(skos:definition dcterms:PeriodOfTime "An interval of time that is named or defined by its start and end dates."@en-us)
AnnotationAssertion(rdfs:label dcterms:PeriodOfTime "Period of Time"@en-us)
SubClassOf(dcterms:PeriodOfTime dcterms:LocationPeriodOrJurisdiction)
Declaration(Class(dcterms:PhysicalMedium))
AnnotationAssertion(rdfs:label dcterms:PhysicalMedium "Physical Medium"@en-us)
AnnotationAssertion(rdfs:comment dcterms:PhysicalMedium "Examples include paper, canvas, or DVD."@en-us)
AnnotationAssertion(skos:definition dcterms:PhysicalMedium "A physical material or carrier."@en-us)
SubClassOf(dcterms:PhysicalMedium dcterms:MediaType)
Declaration(Class(dcterms:PhysicalResource))
AnnotationAssertion(rdfs:label dcterms:PhysicalResource "Physical Resource"@en-us)
AnnotationAssertion(skos:definition dcterms:PhysicalResource "A material thing."@en-us)
Declaration(Class(dcterms:Policy))
AnnotationAssertion(skos:definition dcterms:Policy "A plan or course of action by an authority, intended to influence and determine decisions, actions, and other matters."@en-us)
AnnotationAssertion(rdfs:label dcterms:Policy "Policy"@en-us)
Declaration(Class(dcterms:ProvenanceStatement))
AnnotationAssertion(skos:definition dcterms:ProvenanceStatement "A statement of any changes in ownership and custody of a resource since its creation that are significant for its authenticity, integrity, and interpretation."@en-us)
AnnotationAssertion(rdfs:label dcterms:ProvenanceStatement "Provenance Statement"@en-us)
Declaration(Class(dcterms:RightsStatement))
AnnotationAssertion(skos:definition dcterms:RightsStatement "A statement about the intellectual property rights (IPR) held in or over a Resource, a legal document giving official permission to do something with a resource, or a statement about access rights."@en-us)
AnnotationAssertion(rdfs:label dcterms:RightsStatement "Rights Statement"@en-us)
Declaration(Class(dcterms:SizeOrDuration))
AnnotationAssertion(rdfs:label dcterms:SizeOrDuration "Size or Duration"@en-us)
AnnotationAssertion(rdfs:comment dcterms:SizeOrDuration "Examples include a number of pages, a specification of length, width, and breadth, or a period in hours, minutes, and seconds."@en-us)
AnnotationAssertion(skos:definition dcterms:SizeOrDuration "A dimension or extent, or a time taken to play or execute."@en-us)
SubClassOf(dcterms:SizeOrDuration dcterms:MediaTypeOrExtent)
Declaration(Class(dcterms:Standard))
AnnotationAssertion(skos:definition dcterms:Standard "A basis for comparison; a reference point against which other things can be evaluated."@en-us)
AnnotationAssertion(rdfs:label dcterms:Standard "Standard"@en-us)
Declaration(ObjectProperty(dcam:memberOf))
AnnotationAssertion(skos:definition dcam:memberOf "A relationship between a resource and a vocabulary encoding scheme which indicates that the resource is a member of a set."@en-us)
AnnotationAssertion(rdfs:label dcam:memberOf "Member Of"@en-us)
AnnotationAssertion(rdfs:isDefinedBy dcam:memberOf <http://dublincore.org/documents/2007/06/04/abstract-model/>)
ObjectPropertyRange(dcam:memberOf dcam:VocabularyEncodingScheme)
Declaration(ObjectProperty(dcterms:accessRights))
AnnotationAssertion(rdfs:label dcterms:accessRights "Access Rights"@en-us)
AnnotationAssertion(skos:definition dcterms:accessRights "Information about who can access the resource or an indication of its security status."@en-us)
AnnotationAssertion(rdfs:comment dcterms:accessRights "Access Rights may include information regarding access or restrictions based on privacy, security, or other policies."@en-us)
SubObjectPropertyOf(dcterms:accessRights dcterms:rights)
ObjectPropertyRange(dcterms:accessRights dcterms:RightsStatement)
Declaration(ObjectProperty(dcterms:accrualMethod))
AnnotationAssertion(rdfs:label dcterms:accrualMethod "Accrual Method"@en-us)
AnnotationAssertion(skos:definition dcterms:accrualMethod "The method by which items are added to a collection."@en-us)
ObjectPropertyDomain(dcterms:accrualMethod dcmitype:Collection)
ObjectPropertyRange(dcterms:accrualMethod dcterms:MethodOfAccrual)
Declaration(ObjectProperty(dcterms:accrualPeriodicity))
AnnotationAssertion(skos:definition dcterms:accrualPeriodicity "The frequency with which items are added to a collection."@en-us)
AnnotationAssertion(rdfs:label dcterms:accrualPeriodicity "Accrual Periodicity"@en-us)
ObjectPropertyDomain(dcterms:accrualPeriodicity dcmitype:Collection)
ObjectPropertyRange(dcterms:accrualPeriodicity dcterms:Frequency)
Declaration(ObjectProperty(dcterms:accrualPolicy))
AnnotationAssertion(rdfs:label dcterms:accrualPolicy "Accrual Policy"@en-us)
AnnotationAssertion(skos:definition dcterms:accrualPolicy "The policy governing the addition of items to a collection."@en-us)
ObjectPropertyDomain(dcterms:accrualPolicy dcmitype:Collection)
ObjectPropertyRange(dcterms:accrualPolicy dcterms:Policy)
Declaration(ObjectProperty(dcterms:audience))
AnnotationAssertion(rdfs:label dcterms:audience "Audience"@en-us)
AnnotationAssertion(skos:definition dcterms:audience "A class of entity for whom the resource is intended or useful."@en-us)
ObjectPropertyRange(dcterms:audience dcterms:AgentClass)
Declaration(ObjectProperty(dcterms:conformsTo))
AnnotationAssertion(skos:definition dcterms:conformsTo "An established standard to which the described resource conforms."@en-us)
AnnotationAssertion(rdfs:label dcterms:conformsTo "Conforms To"@en-us)
SubObjectPropertyOf(dcterms:conformsTo dcterms:relation)
ObjectPropertyRange(dcterms:conformsTo dcterms:Standard)
Declaration(ObjectProperty(dcterms:contributor))
AnnotationAssertion(rdfs:label dcterms:contributor "Contributor"@en-us)
AnnotationAssertion(skos:definition dcterms:contributor "An entity responsible for making contributions to the resource."@en-us)
AnnotationAssertion(rdfs:comment dcterms:contributor "Examples of a Contributor include a person, an organization, or a service. Typically, the name of a Contributor should be used to indicate the entity."@en-us)
ObjectPropertyRange(dcterms:contributor dcterms:Agent)
Declaration(ObjectProperty(dcterms:coverage))
ObjectPropertyRange(dcterms:coverage dcterms:LocationPeriodOrJurisdiction)
Declaration(ObjectProperty(dcterms:creator))
AnnotationAssertion(skos:definition dcterms:creator "An entity primarily responsible for making the resource."@en-us)
AnnotationAssertion(rdfs:comment dcterms:creator "Examples of a Creator include a person, an organization, or a service. Typically, the name of a Creator should be used to indicate the entity."@en-us)
AnnotationAssertion(rdfs:label dcterms:creator "Creator"@en-us)
SubObjectPropertyOf(dcterms:creator dcterms:contributor)
ObjectPropertyRange(dcterms:creator dcterms:Agent)
Declaration(ObjectProperty(dcterms:educationLevel))
AnnotationAssertion(skos:definition dcterms:educationLevel "A class of entity, defined in terms of progression through an educational or training context, for which the described resource is intended."@en-us)
AnnotationAssertion(rdfs:label dcterms:educationLevel "Audience Education Level"@en-us)
SubObjectPropertyOf(dcterms:educationLevel dcterms:audience)
ObjectPropertyRange(dcterms:educationLevel dcterms:AgentClass)
Declaration(ObjectProperty(dcterms:extent))
AnnotationAssertion(rdfs:label dcterms:extent "Extent"@en-us)
AnnotationAssertion(skos:note dcterms:extent "In this ontology this property has been defined as an object property, meaning when using this ontology you cannot encode its value as a literal (e.g. by using DCMI's DCSV) but only as a related description."@en)
AnnotationAssertion(skos:definition dcterms:extent "The size or duration of the resource."@en-us)
SubObjectPropertyOf(dcterms:extent dcterms:format)
ObjectPropertyRange(dcterms:extent dcterms:SizeOrDuration)
Declaration(ObjectProperty(dcterms:format))
ObjectPropertyRange(dcterms:format dcterms:MediaTypeOrExtent)
Declaration(ObjectProperty(dcterms:hasFormat))
AnnotationAssertion(skos:definition dcterms:hasFormat "A related resource that is substantially the same as the pre-existing described resource, but in another format."@en-us)
AnnotationAssertion(rdfs:label dcterms:hasFormat "Has Format"@en-us)
SubObjectPropertyOf(dcterms:hasFormat dcterms:relation)
InverseObjectProperties(dcterms:isFormatOf dcterms:hasFormat)
Declaration(ObjectProperty(dcterms:hasVersion))
AnnotationAssertion(skos:definition dcterms:hasVersion "A related resource that is a version, edition, or adaptation of the described resource."@en-us)
AnnotationAssertion(rdfs:label dcterms:hasVersion "Has Version"@en-us)
SubObjectPropertyOf(dcterms:hasVersion dcterms:relation)
InverseObjectProperties(dcterms:isVersionOf dcterms:hasVersion)
Declaration(ObjectProperty(dcterms:has_Part))
AnnotationAssertion(rdfs:label dcterms:has_Part "Has Part"@en-us)
AnnotationAssertion(skos:definition dcterms:has_Part "A related resource that is included either physically or logically in the described resource."@en-us)
SubObjectPropertyOf(dcterms:has_Part dcterms:relation)
InverseObjectProperties(dcterms:is_PartOf dcterms:has_Part)
Declaration(ObjectProperty(dcterms:instructionalMethod))
AnnotationAssertion(rdfs:comment dcterms:instructionalMethod "Instructional Method will typically include ways of presenting instructional materials or conducting instructional activities, patterns of learner-to-learner and learner-to-instructor interactions, and mechanisms by which group and individual levels of learning are measured. Instructional methods include all aspects of the instruction and learning processes from planning and implementation through evaluation and feedback."@en-us)
AnnotationAssertion(skos:definition dcterms:instructionalMethod "A process, used to engender knowledge, attitudes and skills, that the described resource is designed to support."@en-us)
AnnotationAssertion(rdfs:label dcterms:instructionalMethod "Instructional Method"@en-us)
ObjectPropertyRange(dcterms:instructionalMethod dcterms:MethodOfInstruction)
Declaration(ObjectProperty(dcterms:isFormatOf))
AnnotationAssertion(skos:definition dcterms:isFormatOf "A related resource that is substantially the same as the described resource, but in another format."@en-us)
AnnotationAssertion(rdfs:label dcterms:isFormatOf "Is Format Of"@en-us)
SubObjectPropertyOf(dcterms:isFormatOf dcterms:relation)
InverseObjectProperties(dcterms:isFormatOf dcterms:hasFormat)
Declaration(ObjectProperty(dcterms:isReferencedBy))
AnnotationAssertion(skos:definition dcterms:isReferencedBy "A related resource that references, cites, or otherwise points to the described resource."@en-us)
AnnotationAssertion(rdfs:label dcterms:isReferencedBy "Is Referenced By"@en-us)
SubObjectPropertyOf(dcterms:isReferencedBy dcterms:relation)
InverseObjectProperties(dcterms:isReferencedBy dcterms:references)
Declaration(ObjectProperty(dcterms:isReplacedBy))
AnnotationAssertion(rdfs:label dcterms:isReplacedBy "Is Replaced By"@en-us)
AnnotationAssertion(skos:definition dcterms:isReplacedBy "A related resource that supplants, displaces, or supersedes the described resource."@en-us)
SubObjectPropertyOf(dcterms:isReplacedBy dcterms:relation)
InverseObjectProperties(dcterms:isReplacedBy dcterms:replaces)
Declaration(ObjectProperty(dcterms:isRequiredBy))
AnnotationAssertion(skos:definition dcterms:isRequiredBy "A related resource that requires the described resource to support its function, delivery, or coherence."@en-us)
AnnotationAssertion(rdfs:label dcterms:isRequiredBy "Is Required By"@en-us)
SubObjectPropertyOf(dcterms:isRequiredBy dcterms:relation)
InverseObjectProperties(dcterms:isRequiredBy dcterms:requires)
Declaration(ObjectProperty(dcterms:isVersionOf))
AnnotationAssertion(skos:definition dcterms:isVersionOf "A related resource of which the described resource is a version, edition, or adaptation."@en-us)
AnnotationAssertion(rdfs:comment dcterms:isVersionOf "Changes in version imply substantive changes in content rather than differences in format."@en-us)
AnnotationAssertion(rdfs:label dcterms:isVersionOf "Is Version Of"@en-us)
SubObjectPropertyOf(dcterms:isVersionOf dcterms:relation)
InverseObjectProperties(dcterms:isVersionOf dcterms:hasVersion)
Declaration(ObjectProperty(dcterms:is_PartOf))
AnnotationAssertion(skos:definition dcterms:is_PartOf "A related resource in which the described resource is physically or logically included."@en-us)
AnnotationAssertion(rdfs:label dcterms:is_PartOf "Is Part Of"@en-us)
SubObjectPropertyOf(dcterms:is_PartOf dcterms:relation)
InverseObjectProperties(dcterms:is_PartOf dcterms:has_Part)
Declaration(ObjectProperty(dcterms:language))
ObjectPropertyRange(dcterms:language dcterms:LinguisticSystem)
Declaration(ObjectProperty(dcterms:license))
AnnotationAssertion(rdfs:label dcterms:license "License"@en-us)
AnnotationAssertion(skos:definition dcterms:license "A legal document giving official permission to do something with the resource."@en-us)
SubObjectPropertyOf(dcterms:license dcterms:rights)
ObjectPropertyRange(dcterms:license dcterms:LicenseDocument)
Declaration(ObjectProperty(dcterms:mediator))
AnnotationAssertion(rdfs:label dcterms:mediator "Mediator"@en-us)
AnnotationAssertion(rdfs:comment dcterms:mediator "In an educational context, a mediator might be a parent, teacher, teaching assistant, or care-giver."@en-us)
AnnotationAssertion(skos:definition dcterms:mediator "An entity that mediates access to the resource and for whom the resource is intended or useful."@en-us)
SubObjectPropertyOf(dcterms:mediator dcterms:audience)
ObjectPropertyRange(dcterms:mediator dcterms:AgentClass)
Declaration(ObjectProperty(dcterms:provenance))
AnnotationAssertion(rdfs:label dcterms:provenance "Provenance"@en-us)
AnnotationAssertion(skos:definition dcterms:provenance "A statement of any changes in ownership and custody of the resource since its creation that are significant for its authenticity, integrity, and interpretation."@en-us)
AnnotationAssertion(rdfs:comment dcterms:provenance "The statement may include a description of any changes successive custodians made to the resource."@en-us)
ObjectPropertyRange(dcterms:provenance dcterms:ProvenanceStatement)
Declaration(ObjectProperty(dcterms:publisher))
AnnotationAssertion(skos:definition dcterms:publisher "An entity responsible for making the resource available."@en-us)
AnnotationAssertion(rdfs:label dcterms:publisher "Publisher"@en-us)
AnnotationAssertion(rdfs:comment dcterms:publisher "Examples of a Publisher include a person, an organization, or a service. Typically, the name of a Publisher should be used to indicate the entity."@en-us)
ObjectPropertyRange(dcterms:publisher dcterms:Agent)
Declaration(ObjectProperty(dcterms:references))
AnnotationAssertion(rdfs:label dcterms:references "References"@en-us)
AnnotationAssertion(skos:definition dcterms:references "A related resource that is referenced, cited, or otherwise pointed to by the described resource."@en-us)
SubObjectPropertyOf(dcterms:references dcterms:relation)
InverseObjectProperties(dcterms:isReferencedBy dcterms:references)
Declaration(ObjectProperty(dcterms:relation))
AnnotationAssertion(rdfs:label dcterms:relation "Relation"@en-us)
AnnotationAssertion(skos:definition dcterms:relation "A related resource."@en-us)
AnnotationAssertion(rdfs:comment dcterms:relation "Recommended best practice is to identify the related resource by means of a string conforming to a formal identification system."@en-us)
Declaration(ObjectProperty(dcterms:replaces))
AnnotationAssertion(rdfs:label dcterms:replaces "Replaces"@en-us)
AnnotationAssertion(skos:definition dcterms:replaces "A related resource that is supplanted, displaced, or superseded by the described resource."@en-us)
SubObjectPropertyOf(dcterms:replaces dcterms:relation)
InverseObjectProperties(dcterms:isReplacedBy dcterms:replaces)
Declaration(ObjectProperty(dcterms:requires))
AnnotationAssertion(skos:definition dcterms:requires "A related resource that is required by the described resource to support its function, delivery, or coherence."@en-us)
AnnotationAssertion(rdfs:label dcterms:requires "Requires"@en-us)
SubObjectPropertyOf(dcterms:requires dcterms:relation)
InverseObjectProperties(dcterms:isRequiredBy dcterms:requires)
Declaration(ObjectProperty(dcterms:rights))
AnnotationAssertion(rdfs:comment dcterms:rights "Typically, rights information includes a statement about various property rights associated with the resource, including intellectual property rights."@en-us)
AnnotationAssertion(rdfs:label dcterms:rights "Rights"@en-us)
AnnotationAssertion(skos:definition dcterms:rights "Information about rights held in and over the resource."@en-us)
ObjectPropertyRange(dcterms:rights dcterms:RightsStatement)
Declaration(ObjectProperty(dcterms:rightsHolder))
AnnotationAssertion(rdfs:label dcterms:rightsHolder "Rights Holder"@en-us)
AnnotationAssertion(skos:definition dcterms:rightsHolder "A person or organization owning or managing rights over the resource."@en-us)
ObjectPropertyRange(dcterms:rightsHolder dcterms:Agent)
Declaration(ObjectProperty(dcterms:source))
AnnotationAssertion(rdfs:label dcterms:source "Source"@en-us)
AnnotationAssertion(skos:definition dcterms:source "A related resource from which the described resource is derived."@en-us)
AnnotationAssertion(rdfs:comment dcterms:source "The described resource may be derived from the related resource in whole or in part. Recommended best practice is to identify the related resource by means of a string conforming to a formal identification system."@en-us)
SubObjectPropertyOf(dcterms:source dcterms:relation)
Declaration(ObjectProperty(dcterms:spatial))
AnnotationAssertion(skos:note dcterms:spatial "In this ontology this property has been defined as an object property, meaning when using this ontology you cannot encode its value as a literal (e.g. by using DCMI's DCSV) but only as a related description."@en)
AnnotationAssertion(skos:definition dcterms:spatial "Spatial characteristics of the resource."@en-us)
AnnotationAssertion(rdfs:label dcterms:spatial "Spatial Coverage"@en-us)
SubObjectPropertyOf(dcterms:spatial dcterms:coverage)
ObjectPropertyRange(dcterms:spatial dcterms:Location)
Declaration(ObjectProperty(dcterms:subject))
AnnotationAssertion(skos:definition dcterms:subject "The topic of the resource."@en-us)
AnnotationAssertion(rdfs:label dcterms:subject "Subject"@en-us)
AnnotationAssertion(rdfs:comment dcterms:subject "Typically, the subject will be represented using keywords, key phrases, or classification codes. Recommended best practice is to use a controlled vocabulary. To describe the spatial or temporal topic of the resource, use the Coverage element."@en-us)
Declaration(ObjectProperty(dcterms:temporal))
AnnotationAssertion(rdfs:label dcterms:temporal "Temporal Coverage"@en-us)
AnnotationAssertion(skos:note dcterms:temporal "In this ontology this property has been defined as an object property, meaning when using this ontology you cannot encode its value as a literal (e.g. by using DCMI's DCSV) but only as a related description."@en)
AnnotationAssertion(skos:definition dcterms:temporal "Temporal characteristics of the resource."@en-us)
SubObjectPropertyOf(dcterms:temporal dcterms:coverage)
ObjectPropertyRange(dcterms:temporal dcterms:PeriodOfTime)
Declaration(ObjectProperty(dcterms:type))
Declaration(DataProperty(dcterms:abstractText))
AnnotationAssertion(skos:definition dcterms:abstractText "A summary of the resource."@en-us)
AnnotationAssertion(rdfs:label dcterms:abstractText "Abstract"@en-us)
SubDataPropertyOf(dcterms:abstractText dcterms:description)
Declaration(DataProperty(dcterms:alternative))
AnnotationAssertion(skos:definition dcterms:alternative "An alternative name for the resource."@en-us)
AnnotationAssertion(rdfs:comment dcterms:alternative "The distinction between titles and alternative titles is application-specific."@en-us)
AnnotationAssertion(rdfs:label dcterms:alternative "Alternative Title"@en-us)
AnnotationAssertion(skos:note dcterms:alternative "In current practice, this term is used primarily with literal values; however, there are important uses with non-literal values as well. As of December 2007, the DCMI Usage Board is leaving this range unspecified pending an investigation of options."@en-us)
SubDataPropertyOf(dcterms:alternative dcterms:title)
Declaration(DataProperty(dcterms:available))
AnnotationAssertion(skos:definition dcterms:available "Date (often a range) that the resource became or will become available."@en-us)
AnnotationAssertion(rdfs:label dcterms:available "Date Available"@en-us)
SubDataPropertyOf(dcterms:available dcterms:date)
